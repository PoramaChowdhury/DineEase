<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="16dp" />
            <solid android:color="#125A7A" />
            <padding
                android:left="16dp"
                android:top="16dp"
                android:right="16dp"
                android:bottom="16dp" />
            <stroke
                android:width="2dp"
                android:color="#0F4C75" />
        </shape>
    </item>
</selector>
